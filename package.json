{
  "name": "cv",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint --fix .",
    "lint:check": "echo 'checking eslint' && eslint . && echo 'eslint complete\nchecking tsc' && tsc --noEmit && echo 'tsc complete'",
    "generate-pdf": "yarn run build && ts-node --esm generate-pdf.ts"
  },
  "dependencies": {
    "feather-icons": "^4.29.1",
    "normalize.css": "^8.0.1",
    "paper-css": "^0.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/feather-icons": "^4",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "playwright": "^1.41.1",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-singlefile": "^1.0.0"
  }
